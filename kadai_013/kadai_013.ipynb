      "source": [
        "def calculate_price_with_tax(price, tax_rate=0.10):\n",
        "\n",
        "\n",
        "    tax_amount = price * tax_rate\n",
        "\n",
        "    total_price = price + tax_amount\n",
        "    return total_price\n",
        "\n",
        "\n",
        "price = 1000\n",
        "tax_rate = 0.10\n",
        "total_price = calculate_price_with_tax(price, tax_rate)\n",
        "\n",
        "print(f\"商品の税込み価格: {total_price}円\")\n"
